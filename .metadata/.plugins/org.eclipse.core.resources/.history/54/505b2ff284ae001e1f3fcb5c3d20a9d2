package Question;

public class QEight {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
			int[][] array = {
					{95, 86},
					{83,92,96},
					{78,83,93,87,88}
			};
			int sum = 0;
			int i = 0;
			for(int j : array[i]) {
				
				sum += array[i][j];
				
			}
			
	}

}
