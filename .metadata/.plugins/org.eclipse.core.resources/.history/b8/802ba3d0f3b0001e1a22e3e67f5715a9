package ch08;

public class SealedExam {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		ImplClass impl = new ImplClass();
		
		InterfaceA ia = impl;
		impl.methodA();
		impl.methodC();
		
		
		InterfaceB ib = impl;
		impl.methodA();
		impl.methodB();
		impl.methodC();
		
		
		InterfaceC ic = impl;
		
		
		
		impl.methodA();
		impl.methodB();
		impl.methodC();
	}

}
