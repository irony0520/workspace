package ch06;

class Account {
	
	private int balance;
	
	public int getBalance() {
		return this.balance;
		
	}

	
	public void setBalance(int money) {
		if(money>=1000) {
			this.balance = money;
		
		}
		
	}
	
	public int withdraw(int money) {

		this.balance -= money;
		return this.balance;
	}
	
	
	
	
}
