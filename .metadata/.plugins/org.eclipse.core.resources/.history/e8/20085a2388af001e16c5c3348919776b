package ch06.example;

public class Exercise6_24 {
	
	int abs(int value) {
		return (int) Math.sqrt(this.value*this.value);
		
	}

	
	
	
	public static void main(String[] ars)
	{
		int value = 5;
		System.out.println(value+"의 절대값:" + abs(value));
		value = -10;
		System.out.println(value+"의 절대값:" + abs(value));
	}
	
	
	
}
