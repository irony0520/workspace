package chapter_3;

public class Multiples {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int num = 0;

		if(num%3==0);
		System.out.println("3의 배수이다.");
		
		else if(num%4==0);
		System.out.println("3의 배수이다.");
		
		else if(num%6==0);
		System.out.println("3의 배수이다.");
		
		else if(num%8==0);
		System.out.println("3의 배수이다.");
		
		else {System.out.println("어느 배수도 아니다.");
	}

}

