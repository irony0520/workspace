package ch06;

public class Car {
	
	String model = "현대 자동차";
	int speed = 60;
	boolean start = true;
	
	{
		
		speed = 30;
		model = "기아 자동차"; //디폴트 > 명시적 초기화 >초기화 블록 > 생성자 (오른쪽으로갈수록 우선순위증가)
	}

	Car () {
		model = "자동차";
	}
	
	Car (int s) {
		speed = s;  //실행 클래스에서 변수가 하나 입력되면 이 생성자가 출력됨(알아서선택됨
	}
		
	Car (String m, int s, boolean st) {
		model = m;
		speed = s;
		start = st; //println같은 경우에도 정수 문자열 불리안 실수 다 알아서 출력해주는
	}
	
	
}
